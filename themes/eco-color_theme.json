	"tokenColors": [
		{
			"settings": {
				"background": "#1e1e1e",
				"foreground": "#ABB2BF",
				"caret": "#528BFF",
				"invisibles": "#3C4049",
				"lineHighlight": "#2C313A",
				"selection": "#3E4451",
				"findHighlight": "#FFE792",
				"guide": "#3C4049",
				"activeGuide": "#CACED6"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.modifier.rust"
			],
			"settings": {
				"foreground": "#7ca1b9"
			}
		},
		{
			"scope": [
				"comment",
				"comment.line.double-slash.go",
				"punctuation.definition.comment.go"
			],
			"settings": {
				"foreground": "#c4c5a27a"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.language",
				"punctuation.definition.interpolation.rust"
			],
			"settings": {
				"foreground": "#b092a5"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function.go",
				"support.type.property-name.table.toml",
				"punctuation.definition.table.toml",
				"keyword.control.at-rule.media.css"
			],
			"settings": {
				"foreground": "#cc6666"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"storage.type.rust",
				"meta.attribute.rust",
				"variable.language.rust",
				"storage.type.module.rust",
				"storage.modifier.mut.rust",
				"support.type.property-name.json",
				"support.type.property-name.json punctuation",
				"storage.modifier.sql",
				"storage.modifier.java",
				"punctuation.definition.entity.css",
				"entity.other.attribute-name.class.css",
				"entity.name.tag.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.pseudo-class.css",
				"support.type.property-name.toml",
				"storage.modifier.async.tsx"
			],
			"settings": {
				"foreground": "#7fa1bd"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"storage.type.sql",
				"storage.type.string.go",
				"storage.type",
				"meta.at-rule.media.header.css"
			],
			"settings": {
				"foreground": "#8abeb7"
			}
		},
		{
			"scope": [
				"variable.other",
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#eaeaead0"
			}
		},
		{
			"scope": "entity.name.type.mod.rust",
			"settings": {
				"foreground": "#D4BE98"
			}
		},
		{
			"scope": [
				"source.rust keyword.control",
				"keyword.control.go"
			],
			"settings": {
				"foreground": "#bd7f7f"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"meta.property-value.css",
				"meta.property-list.css"
			],
			"settings": {
				"foreground": "#c4c5a2"
			}
		}
	]
}